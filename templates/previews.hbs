<div id="pf2ett-super-container">
    <div id="pf2ett-example-traits">
        <tags class="tagify paizo-style tags">
            {{#each sampleTraits}}
                <tag id="{{dashedName}}" class="tagify__tag tagify--noAnim" data-tooltip="{{localize tooltipKey}}">
                    <div><span class="tagify__tag-text">{{localize nameKey}}</span></div>
                </tag>
            {{/each}}
        </tags>
        <br/>
        <tags class="tagify paizo-style tags">
            {{#each allTheTraits}}
                <tag id="{{dashedName}}" class="tagify__tag tagify--noAnim" data-tooltip="{{localize tooltipKey}}">
                    <div><span class="tagify__tag-text">{{localize nameKey}}</span></div>
                </tag>
            {{/each}}
        </tags>
    </div>
</div>